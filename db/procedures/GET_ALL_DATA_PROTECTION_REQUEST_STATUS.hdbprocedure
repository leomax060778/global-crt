PROCEDURE "MKTG_CART_REQUEST_TOOL"."mktgcartrequesttool.db.procedures::GET_ALL_DATA_PROTECTION_REQUEST_STATUS" (
	OUT out_result TABLE (STATUS_ID BIGINT,
						  STATUS_NAME NVARCHAR(255)
						 )
) 
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER 
	DEFAULT SCHEMA "MKTG_CART_REQUEST_TOOL"
	READS SQL DATA AS
BEGIN
	out_result = SELECT USER_DATA_PROTECTION_STATUS_ID AS STATUS_ID,
				 		NAME AS STATUS_NAME
				 FROM "USER_DATA_PROTECTION_STATUS"
				 WHERE ENABLED = 1 AND DELETED = 0;
END;

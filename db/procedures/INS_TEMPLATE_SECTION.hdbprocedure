PROCEDURE "MKTG_CART_REQUEST_TOOL"."mktgcartrequesttool.db.procedures::INS_TEMPLATE_SECTION" (
	IN in_name NVARCHAR(255),
	IN in_section_order INTEGER,
	IN in_created_user_id BIGINT,
	OUT out_result bigint
 ) 
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER 
	DEFAULT SCHEMA "MKTG_CART_REQUEST_TOOL"
	AS
BEGIN
INSERT INTO "MKTG_CART_REQUEST_TOOL"."TEMPLATE_SECTION"(NAME, SECTION_ORDER, CREATED_USER_ID)
  VALUES(in_name, in_section_order, in_created_user_id);
 
  SELECT TOP 1 CURRENT_IDENTITY_VALUE() INTO out_result FROM "MKTG_CART_REQUEST_TOOL"."TEMPLATE_SECTION";
END;

PROCEDURE "MKTG_CART_REQUEST_TOOL"."mktgcartrequesttool.db.procedures::GET_CHANGE_VENDOR_REQUEST_STATUS_BY_ID" (
	IN in_status_id bigint,
	OUT out_result TABLE (status_id bigint, name nvarchar(255), administrable tinyint)
 ) 
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER 
	DEFAULT SCHEMA "MKTG_CART_REQUEST_TOOL"
	READS SQL DATA AS
BEGIN
	out_result = SELECT status_id, name, administrable
		FROM "CHANGE_VENDOR_REQUEST_STATUS"
		WHERE status_id = in_status_id AND ENABLED = 1 AND DELETED = 0;
END;

PROCEDURE "MKTG_CART_REQUEST_TOOL"."mktgcartrequesttool.db.procedures::GET_ATTACHMENT_INQUIRY_BY_ATTACHMENT_ID" (
	IN in_attachment_id bigint,
	OUT out_result TABLE (inquiry_id bigint)
 ) 
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER 
	DEFAULT SCHEMA "MKTG_CART_REQUEST_TOOL"
	READS SQL DATA AS
BEGIN
	out_result = SELECT INQUIRY_ID FROM "ATTACHMENT_INQUIRY"
	 WHERE in_attachment_id = "ATTACHMENT_INQUIRY".ATTACHMENT_ID
	  AND DELETED = 0
	  AND ENABLED = 1;
END;

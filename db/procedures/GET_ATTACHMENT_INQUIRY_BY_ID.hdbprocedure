PROCEDURE "MKTG_CART_REQUEST_TOOL"."mktgcartrequesttool.db.procedures::GET_ATTACHMENT_INQUIRY_BY_ID" (
	IN in_inquiry_id bigint,
	OUT out_result TABLE (attachment_id bigint)
 ) 
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER 
	DEFAULT SCHEMA "MKTG_CART_REQUEST_TOOL"
	READS SQL DATA AS
BEGIN
	out_result = SELECT ATTACHMENT_ID FROM "ATTACHMENT_INQUIRY"
	 WHERE in_inquiry_id = "ATTACHMENT_INQUIRY".INQUIRY_ID
	  AND DELETED = 0
	  AND ENABLED = 1;
END;

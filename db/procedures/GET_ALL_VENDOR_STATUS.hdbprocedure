PROCEDURE "MKTG_CART_REQUEST_TOOL"."mktgcartrequesttool.db.procedures::GET_ALL_VENDOR_STATUS" (
	OUT out_result TABLE (
		STATUS_ID bigint, 
		NAME nvarchar(255)
	)
 ) 
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER 
	DEFAULT SCHEMA "MKTG_CART_REQUEST_TOOL"
	READS SQL DATA AS
BEGIN
	out_result = SELECT 
		"VENDOR_STATUS".STATUS_ID, 
		"VENDOR_STATUS".NAME
	FROM "VENDOR_STATUS"
	WHERE "VENDOR_STATUS".enabled = 1 
		AND "VENDOR_STATUS".deleted = 0;
END;

PROCEDURE "MKTG_CART_REQUEST_TOOL"."mktgcartrequesttool.db.procedures::GET_ALL_REQUEST_STATUS" (
	OUT out_result TABLE (status_id bigint, name nvarchar(255))
 ) 
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER 
	DEFAULT SCHEMA "MKTG_CART_REQUEST_TOOL"
	READS SQL DATA AS
BEGIN
	out_result = SELECT RST.STATUS_ID, RST.NAME
					FROM "REQUEST_STATUS" RST
				 WHERE RST.ENABLED = 1 
					 AND RST.DELETED = 0
				 ORDER BY RST.STATUS_ID ASC;
END;

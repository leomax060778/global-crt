PROCEDURE "MKTG_CART_REQUEST_TOOL"."mktgcartrequesttool.db.procedures::GET_ALL_TEMPLATE_TYPE" (
	OUT out_result TABLE ( template_type_id bigint, name nvarchar(255))
 ) 
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER 
	DEFAULT SCHEMA "MKTG_CART_REQUEST_TOOL"
	READS SQL DATA AS
BEGIN
	out_result = SELECT "TEMPLATE_TYPE".TEMPLATE_TYPE_ID, "TEMPLATE_TYPE".NAME
	FROM "TEMPLATE_TYPE"
		WHERE deleted = 0
		AND enabled = 1;
END;